# Environment Variables สำหรับ Bamboo Platform
# คัดลอกไฟล์นี้เป็น .env และใส่ค่าจริง

# Gemini AI API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Environment
NODE_ENV=development

# API Base URL (ถ้ามี)
API_BASE_URL=https://api.bamboo-platform.com

# Other configurations
VITE_APP_TITLE=Bamboo Platform
VITE_APP_VERSION=1.0.0
